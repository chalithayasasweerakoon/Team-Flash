<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.expensemanager.teamflash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon_round"
        android:label="@string/header"
        android:roundIcon="@mipmap/ic_launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.expensemanager.teamflash.splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.expensemanager.teamflash.transportsum"
            android:label="@string/transportsum_str"
            android:parentActivityName="com.expensemanager.teamflash.dashboard"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.expensemanager.teamflash.transportedit"
            android:label="@string/title_activity_transportedit"
            android:parentActivityName="com.expensemanager.teamflash.transportsum"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.transport"
            android:label="@string/transport_str"
            android:parentActivityName="com.expensemanager.teamflash.transportsum"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.expensemanager.teamflash.dashboard"
            android:label="@string/dashboard"
            android:parentActivityName="com.expensemanager.teamflash.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.food"
            android:label="@string/food_str"
            android:parentActivityName="com.expensemanager.teamflash.foodsum"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.shopping"
            android:label="@string/shopping_str"
            android:parentActivityName="com.expensemanager.teamflash.shoppingsum"
            android:screenOrientation="portrait" /> <!-- <activity -->
        <activity
            android:name="com.expensemanager.teamflash.summary"
            android:label="@string/summary_str"
            android:parentActivityName="com.expensemanager.teamflash.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.foodsum"
            android:label="@string/foodsum_str"
            android:parentActivityName="com.expensemanager.teamflash.dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.shoppingsum"
            android:label="@string/shoppingsum_str"
            android:parentActivityName="com.expensemanager.teamflash.dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.MainActivity"
            android:label="Main Activity"
            android:parentActivityName="com.expensemanager.teamflash.dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.foodedit"
            android:label="@string/title_activity_foodedit"
            android:parentActivityName="com.expensemanager.teamflash.foodsum"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.expensemanager.teamflash.shoppingedit"
            android:label="@string/title_activity_shoppingedit"
            android:parentActivityName="com.expensemanager.teamflash.shoppingsum"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>